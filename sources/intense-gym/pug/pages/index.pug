            "type": "thumbnail",
            "figcaption": true,
            "title": "circle studio",
            "description": null,
            "thumb": {"src": "images/home-04-480x480.jpg", "width": 480, "height": 480, "alt": ""},
            "target": {"href": cfg.nav.schedule.href},
            "btnClass": "btn-danger",
            "btnLink": cfg.nav.schedule.href,
