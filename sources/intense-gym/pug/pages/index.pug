    // Testimonials
    section
      +material-parallax({ img: 'images/background-05-1920x850.jpg' })
        .bg-overlay-gray-darkest.context-dark
          .container.section-98.section-md-110
            h1 Testimonials
            hr.divider.bg-white
            .row.justify-content-sm-center
              .col-md-8
                +testimonials-slider-3([{
                  "image": {
                    "width": 80,
