extends  ../_skeleton

//- Includes
include ../elements/_ui-page-loader-variant-1
include  ../elements/_ui-box-icon-type-5
include ../elements/_ui-thumbnail-terry
include ../elements/_ui-box-member
include ../elements/_widget-list-social
include ../elements/_ui-post-boxed
include ../elements/_ui-material-parallax
include ../elements/_ui-testimonials-slider-3

//-Title
block title
  title= cfg.brand.name

//- Page
block page

  +page-loader-1(cfg.brand.logoBig)

  // Page Head
  header.page-head.slider-menu-position
    //- RD Navbar
    +rd-navbar-floated(
      'rd-navbar-dark',
      {
        "data-lg-auto-height": "true",
        "data-md-layout": 'rd-navbar-fixed',
        "data-lg-layout": 'rd-navbar-static',
        "data-lg-stick-up": 'true'
      },
      cfg.brand.logo,
      cfg.nav.home,
      cfg.nav
    )
    include ../sections/_section-swiper


  // Page Contents
  main.page-content

    //Welcome
    section.section-98.section-md-110#welcome
      .container
        h1 Welcome
        hr.divider.bg-red
        .row.justify-content-sm-center.offset-top-66
          .col-xl-8
            p Welcome to the website Intense Gym! We hope that you will appreciate our services and opportunities we offer our loyal and potential customers. Here are some of them:
        .row.justify-content-center.grid-group-lg.offset-top-98
          .col-md-8.col-lg-4
            +box-icon-type-5("icon-sm icon-dark-filled mdi mdi-account-multiple", "Qualified Coaches", "Our coaches have years of experience in various types of fitness and sports.")
          .col-md-8.col-lg-4
            +box-icon-type-5("icon-xs icon-dark-filled mdi mdi-thumb-up", "Individual Approach", "Every client of Intense has a personalized program of training and nutrition.")
          .col-md-8.col-lg-4
            +box-icon-type-5("icon-sm icon-dark-filled mdi mdi-dumbbell", "Modern Fitness Equipment", "We cooperate with leading fitness equipment suppliers to give you the superior results.")

    // Offers
    section
      .owl-carousel.owl-carousel-default.d-lg-none-owl-dots.veil-owl-nav.d-lg-owl-nav(data-items="1" data-sm-items="2" data-lg-items="3" data-xl-items="4" data-nav="true" data-dots="true" data-nav-class='["owl-prev mdi mdi-chevron-left", "owl-next mdi mdi-chevron-right"]')
        div
          +thumbnail-terry({
            "type": "thumbnail",
            "figcaption": true,
            "title": "Bodybuilding",
            "description": null,
            "thumb": {"src": "images/home-01-480x480.jpg", "width": 480, "height": 480, "alt": ""},
            "target": {"href": cfg.nav.schedule.href},
            "btnClass": "btn-danger",
            "btnLink": cfg.nav.schedule.href,
            "btnText": "free first lesson"
          })
        div
          +thumbnail-terry({
            "type": "thumbnail",
            "figcaption": true,
            "title": "fitness",
            "description": null,
            "thumb": {"src": "images/home-02-480x480.jpg", "width": 480, "height": 480, "alt": ""},
            "target": {"href": cfg.nav.schedule.href},
            "btnClass": "btn-danger",
            "btnLink": cfg.nav.schedule.href,
            "btnText": "free first lesson"
          })
        div
