// RD Search
.rd-navbar-search.rd-navbar-search_toggled.rd-navbar-search_not-collapsable
  .rd-navbar-search-inner
  form.rd-search( method="GET", data-search-live='rd-search-results-live')
    .form-wrap
      label.form-label(for="rd-navbar-search-form-input") Search...
      input.form-input(id="rd-navbar-search-form-input" type="text" name="s" autocomplete="off" )
    button.rd-search__submit(type='submit')