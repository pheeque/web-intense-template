  //Our Services
  section.section.novi-background.section-navigation.section-80.bg-gray-lighter
    .navigation-wrap
      nav.navigation
        ul.navigation-list
          li: a( href='#', data-custom-scroll-to='home' ) Home
          li: a( href='#', data-custom-scroll-to='features' ) Features
          li: a( href='#', data-custom-scroll-to='responsive' ) Responsive
          li.active: a( href='#', data-custom-scroll-to='help' ) Help
          li: a( href='#', data-custom-scroll-to='testimonials' ) Testimonials

    .container
      .row.grid-group-md.justify-content-between
        .col-lg-5.text-lg-left
          .offset-lg-top-30
