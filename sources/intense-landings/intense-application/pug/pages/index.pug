          .col-md-6.text-md-left
            a( href='../../index.html' )
              img.img-fluid.d-inline-block( src='images/logo-ligth-183x32.png', width='183', height='32', alt='' )
          .col-md-6
            ul.list-inline.text-right.list-inline-md.list-inline-primary
              li.list-inline-item: a.novi-icon.icon.icon-xxs.fa.fa-facebook( href='#' )
