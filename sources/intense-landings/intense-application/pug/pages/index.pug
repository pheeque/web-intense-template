            .rd-navbar-panel
              // RD Navbar Toggle
              button.rd-navbar-toggle( data-rd-navbar-toggle='.rd-navbar, .rd-navbar-nav-wrap' ): span
              .rd-navbar-brand
                a( href='./' )
                  img.img-fluid.d-inline-block( src='images/navbar-logo-ligth-183x32.png', width='183', height='32', alt='' )
            .rd-navbar-menu-wrap
              .rd-navbar-nav-wrap
                .rd-navbar-mobile-scroll
                  // RD Navbar Nav
                  ul.rd-navbar-nav
                    li.active: a( href='#home' ) Home
                    li: a( href='#features' ) Features
                    li: a( href='#responsive' ) Design
                    li: a( href='#help' ) Support
                    li: a( href='#testimonials' ) Testimonials
  
