extends  ../_skeleton

//-Title
block title
  title= cfg.nav.blogArchive.title

block search-meta
  meta(name="keywords" content="My Publications")
  meta(name="date" content="Feb 18")

//- Page
block page
  // Page Head
  header.page-head
    //- RD Navbar
    +rd-navbar-sidebar-fixed(
    'rd-navbar-light rd-navbar-sidebar-fixed',
    {
      "data-lg-auto-height": "false",
      "data-lg-stick-up": 'false',
      "data-lg-hover-on": "false",
      "data-body-class": "body-inset-left"
    },
    cfg.brand.logo,
    cfg.nav.blogArchive,
    cfg.nav
    )

  // Page Content
  main
    // Header Logo
    include ../sections/_section-header-logo

    // Page Content
    .page-content.section-top-124.section-bottom-98.section-sm-top-0.section-xl-bottom-124.offset-sm-top-41
      // My Publications
      .container
        h2.font-weight-bold
          span.h6.d-block.text-uppercase.text-regular.font-default.text-spacing-120.text-primary blog
          | My Publications
        .row.justify-content-md-center.justify-content-xl-start
          .col-lg-9.inset-lg-right-30
            include ../sections/_section-blog-timeline-archive
          .col-lg-3.col-xl-3.offset-top-66.offset-lg-top-0
            include ../sections/_section-blog-sidebar-variant-6

  // Page Footers
  include ../sections/_section-footer-variant-3