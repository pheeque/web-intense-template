extends  ../_skeleton

//- Includes
include  ../elements/_ui-tabs-horizontal-dashed
include  ../elements/_ui-thumbnail-terry
include  ../elements/_ui-material-parallax
include  ../elements/_ui-box-icon-type-4
include  ../elements/_ui-testimonials-slider-4

//-Title
block title
  title= cfg.brand.name

//- Page
block page

  // Page Head
  header.page-head
    //- RD Navbar
    +rd-navbar-top-panel(
    'rd-navbar-light',
    {
      "data-lg-auto-height": "true",
      "data-md-layout": 'rd-navbar-fixed',
      "data-lg-layout": 'rd-navbar-static',
      "data-lg-stick-up": 'true'
    },
    cfg.brand.logo,
    cfg.nav.home,
    cfg.nav
    )

  // Page Contents
  main.page-content
    //Section Search form
    section.context-dark
      // Swiper
      .swiper-container.swiper-slider(data-height='33%', data-min-height='600px', data-dots="true")
        .swiper-wrapper
          .swiper-slide.swiper-slide-overlay-disable(data-slide-bg='images/slide-01-1920x640.jpg' style="background-position: center center")
          .swiper-slide.swiper-slide-overlay-disable(data-slide-bg='images/slide-02-1920x640.jpg' style="background-position: center center")
          .swiper-slide.swiper-slide-overlay-disable(data-slide-bg='images/slide-03-1920x640.jpg' style="background-position: center center")
        .swiper-caption-wraper
          .container.section-41
            .row.justify-content-sm-center
              h1.font-weight-bold Professional realtor works for you
              .col-sm-10.col-lg-7.col-xl-6
                .offset-top-24.offset-lg-top-50.text-left
                  +tabs-horizontal-dashed(['sale ', 'rent', 'commercial'])
                    div.offset-lg-top-10
                      form
                        .group-sm.group-top
                          .group-item.element-fullwidth(style="max-width: 390px")
                            .form-group
                              label.form-label.rd-input-label(for="home-tabs1-search-form-input") Search by City or Address:
                              input.form-control#home-tabs1-search-form-input(type="text" name="s" autocomplete="off")
                          button.btn.btn-primary.element-fullwidth(type="button" style="max-width: 120px") search
                    div.offset-lg-top-10
                      form
                        .group-sm.group-top
                          .group-item.element-fullwidth(style="max-width: 390px")
                            .form-group
                              label.form-label.rd-input-label(for="home-tabs2-search-form-input") Search by  Address or City:
                              input.form-control#home-tabs2-search-form-input(type="text" name="s" autocomplete="off")
                          button.btn.btn-primary.element-fullwidth(type="button" style="max-width: 120px") search
                    div.offset-lg-top-10
                      form
                        .group-sm.group-top
                          .group-item.element-fullwidth(style="max-width: 390px")
                            .form-group
                              label.form-label.rd-input-label(for="home-tabs3-search-form-input") Search by City or Address:
                              input.form-control#home-tabs3-search-form-input(type="text" name="s" autocomplete="off")
                          button.btn.btn-primary.element-fullwidth(type="button" style="max-width: 120px") search

        // Swiper Pagination
        .swiper-pagination.swiper-pagination-type-2

